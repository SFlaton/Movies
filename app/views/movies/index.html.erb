<div class='row'>
    <% @movies.each do |movie| %>
        <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
          <div class="caption">
            <h3><%= movie.title %></h3>
            <p><%= link_to 'Show', category_movie_path, class: "btn btn-default" %> </p>
          </div>
        </div>
      </div>
    <% end %>
  </div>
